User
endpoint /users

POST /
returns User

GET /id/:id?
returns User

PUT /
returns User

DELETE /:id
-

POST /signin
returns User

GET /logout
returns 204 EMPTY


Location
endpoint /locations

POST /
returns Location

GET /
query {
  plantcount: boolean: number
  plants: boolean: Plant[]
  limit: number: Plant[]
}
returns Location[] + Plant[]?

GET /:id
query {
  plants: boolean: Plant[]
  limit: number: Plant[]
}
returns Location + Plant[]?

GET /user/:userId
query {
  plantcount: boolean: number
  plants: boolean: Plant[]
  limit: number: Plant[]
}
returns Location[] + Plant[]?

PUT /
returns Location

DELETE /:id
returns 204 EMPTY


Plant
endpoint /plants

POST /
returns Plant

GET /
query {
  photos: boolean: Photo[]
  cover: boolean: Photo
}
returns Plant[] + Specie + Photo[]?

GET /user/:userId
query {
  photos: boolean: Photo[]
  cover: boolean: Photo
}
returns Plant[] + Specie + Photo[]?

GET /user/:userId/location/:locationId
query {
  photos: boolean: Photo[]
  cover: boolean: Photo
}
returns Plant[] + Specie + Photo[]?

GET /location/:locationId
query {
  photos: boolean: Photo[]
  cover: boolean: Photo
}
returns Plant[] + Specie + Photo[]?

GET /:id
query {
  photos: boolean: Photo[]
  cover: boolean: Photo
}
returns Plant + Photo[]?

GET /:id/cover
returns coverId

PUT /
returns Plant

DELETE /:id
returns 204 EMPTY


Photo
endpoint /photos

POST /
returns BackendResponse

GET /
returns Photo[]

GET /plant/:plantId
returns Photo[]

GET /:id
returns Photo

GET /:id/navigation
returns navigation

PUT /
returns Photo

DELETE /
returns 204 EMPTY


Specie
endpoint /species

POST /
returns BackendResponse

GET /name/:name?
returns Specie[]

GET /:id
returns Specie

PUT /
returns BackendResponse

DELETE /:id
returns BackendResponse


Task
endpoint /tasks

GET /
returns Plant[]