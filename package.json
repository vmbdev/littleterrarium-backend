    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "prisma": "^4.14.0",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
